{
  "name": "gridsome-headlesswp",
  "version": "1.0.0",
  "browserslist": "> 0.5%, last 2 versions, IE 10",
  "description": "Gridsome Boilerplate using WordPress for content",
  "author": "Alistair Shepherd <alistair@accudio.com> (https://alistairshepherd.uk/)",
  "contributors": [
    "Alistair Shepherd <alistair@accudio.com> (https://alistairshepherd.uk/)"
  ],
  "private": false,
  "license": "MPL-2.0",
  "repository": "github:accudio/gridsome-headlesswp",
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "generate": "gridsome build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . && stylelint **/*.scss **/*.sass **/*.vue --ignore-path .gitignore",
    "fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix && stylelint **/*.scss **/*.sass **/*.vue --ignore-path .gitignore --fix",
    "precommit": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "@gridsome/plugin-sitemap": "^0.2.2",
    "axios": "^0.19.0",
    "camelcase": "^5.3.1",
    "fs": "0.0.1-security",
    "gridsome": "^0.7.10",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "p-map": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "stylelint-scss": "^3.12.0",
    "stylelint-webpack-plugin": "^1.0.3",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-resources-loader": "^1.2.1"
  }
}
